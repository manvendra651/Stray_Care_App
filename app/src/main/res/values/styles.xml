<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="title_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingStart">20dp</item>
        <item name="android:paddingEnd">20dp</item>
    </style>

    <style name="title_text_style">
        <item name="android:layout_width">97dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:paddingStart">10dp</item>
        <item name="android:textColor">#090808</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="dashboard_button">
        <item name="android:layout_width">200dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_marginStart">104dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginEnd">106dp</item>
        <item name="android:backgroundTint">#07504F</item>
    </style>

    <style name="container_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">#0E0E0E</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Conatiner_data">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">#304FFE</item>
        <item name="android:textSize">16sp</item>
    </style>
</resources>